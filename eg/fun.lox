fun very_slow_exp(a, n) {
    var r = 1;
    for (var i = 0; i < n; i = i + 1) r = r * a;
    return r;
}

print very_slow_exp(3, 3);

print very_slow_exp(3,4) == very_slow_exp(9,2);
print very_slow_exp(1,3) == very_slow_exp(3,1);

fun plus(a, b) {
    return a + b;
}

print plus(
    plus(5, 5), 
    plus(5, 5)
);

print plus(
    plus("Hello", ", "), 
    plus("World", "!")
); 
